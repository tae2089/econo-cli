{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["🚀", "🌟"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["🐛"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["🧪"]
    },
    {
      "title": "## 💬 Other",
      "labels": ["💬", "📖", "🚨"]
    },
    {
      "title": "## 📦 Dependencies",
      "labels": ["📦"]
    }
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}}\n   - PR: #${{NUMBER}}",
  "label_extractor": [
    {
      "pattern": "(.) (.+)",
      "target": "$1"
    }
  ],
  "transformers": [
    {
      "pattern": "(.) (.+)",
      "target": "- $2"
    }
  ],
  "empty_template": "- no changes",
  "max_pull_requests": 1000,
  "max_back_track_time_days": 1000
}
